{
  "name": "fastify-mysql-sequelize-server",
  "version": "1.0.1",
  "description": "A production ready backend server created on fastify and mysql",
  "module": "src/app.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "rimraf dist && bun build --entrypoints ./src/app.ts --outdir ./dist --format esm --splitting --minify --target node",
    "prestart": "bun run build",
    "start": "bun ./dist/app.js",
    "dev": "bun run --watch ./src/app.ts",
    "prettier": "prettier --write ."
  },
  "devDependencies": {
    "@types/bun": "latest",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "dotenv": "^16.5.0",
    "fastify": "^5.3.0",
    "fastify-plugin": "^5.0.1",
    "mysql2": "^3.14.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7"
  },
  "keywords": [
    "fastify",
    "mysql",
    "sequelize",
    "fastify-plugin"
  ]
}
